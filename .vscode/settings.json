{
  "yaml.schemas": {
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": ["temporal", "kv"],
  "editor.formatOnSave": true,
  // "editor.formatOnSave": false,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "editor.tabSize": 2,
  "editor.tabCompletion": "on"
}
